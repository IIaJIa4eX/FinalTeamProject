@using DatabaseConnector;
@using FinalProject.Services;
@using Microsoft.AspNetCore.Identity

@{
    ViewData["Title"] = "Registration";
}

<ul class="navbar-nav">
@if (1<0)
{
    <li class="nav-item">
        <a  class="nav-link text-dark" asp-area="" asp-page="/Account/Manage/Index" title="Manage">@User.Identity?.Name!</a>
    </li>
    <li class="nav-item">
        <form class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Page("/", new { area = "" })" method="post" >
            <button  type="submit" class="nav-link btn btn-link text-dark">Logout</button>
        </form>
    </li>
}
else
{
    <li class="nav-item">
            <a class="nav-link text-dark" asp-controller="User" asp-action="Registration" method="get">Register</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-page="/Account/Login">Login</a>
    </li>
}
</ul>

