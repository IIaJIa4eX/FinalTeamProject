@model List<DatabaseConnector.User>

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Пользователи</h1>

@foreach (var model in Model)
{
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(m => model.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(m => model.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(m => model.NickName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(m => model.NickName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(m => model.FirstName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(m => model.FirstName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(m => model.LastName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(m => model.LastName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(m => model.Patronymic)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(m => model.Patronymic)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(m => model.Birthday)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(m => model.Birthday)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(m => model.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(m => model.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(m => model.IsBanned)
        </dt>
        <dd class="col-sm-10">
            @Html.CheckBoxFor(m => model.IsBanned)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(m => model.UserRole)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(m => model.UserRole)
        </dd>
    </dl>

    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
}
@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}

